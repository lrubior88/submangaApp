#!/usr/bin/env python
# -*- coding: cp1252 -*-
 
import httplib # Necesaria para hacer peticiones HTTP
import os
 
# Configuraci√≥n

conn = httplib.HTTPConnection("submanga.com")
peticion = "/c/180109"
conn.request("GET", peticion)

r = conn.getresponse()
print r.status, r.reason
data = r.read()
print data
